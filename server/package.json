{
  "name": "student-itrack-server",
  "version": "1.0.0",
  "description": "Express.js backend server for Student iTrack application",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "start:silent": "node src/server.js 2>&1 | grep -v 'npm warn config production' || node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "node scripts/migrate-firestore-to-mysql.js",
    "test-db": "node scripts/test-db-connection.js",
    "setup-db": "node scripts/setup-database.js"
  },
  "keywords": [
    "student",
    "tracking",
    "academic",
    "express",
    "mysql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.3.1",
    "firebase-admin": "^12.0.0",
    "mysql2": "^3.6.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}

