    "tracking",
    "academic",
    "fastify",
    "mysql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "fastify-plugin": "^4.5.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "pino-pretty": "^10.2.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  },
  "engines": {
    "node": ">=14.0.0"
  }
