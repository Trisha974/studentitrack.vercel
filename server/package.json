{
  "name": "student-itrack-server",
  "version": "1.0.0",
  "description": "Fastify backend server for Student iTrack application",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "start:silent": "node src/server.js 2>&1 | grep -v 'npm warn config production' || node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "node scripts/migrate-firestore-to-mysql.js",
    "test-db": "node scripts/test-db-connection.js",
    "setup-db": "node scripts/setup-database.js"
